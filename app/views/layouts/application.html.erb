<!DOCTYPE html>
<html>
<head>
  <title>WebGeekDevCup</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header>
		<div class="row">
			<%=	render 'layouts/header' %>
		</div>
	</header>
	<%= render 'layouts/messages' %>
	<div class="row">
		<div class="twelve columns">
			<%= yield %>
		</div>
	</div>
	<footer>
		<%= render 'layouts/footer' %>
	</footer>
</body>
</html>
